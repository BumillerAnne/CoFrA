<?xml version="1.0" encoding="UTF-8"?>
<modellingFrameworkContext4Authentication:ModellingFramework
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:modellingFrameworkContext4Authentication="http://www.example.org/modellingFrameworkContext4Authentication"
    xsi:schemaLocation="http://www.example.org/modellingFrameworkContext4Authentication ../modellingFrameworkContext4Authentication.ecore">
  <contextinformation
      xsi:type="modellingFrameworkContext4Authentication:RequiredContextInformation"
      context="NearbyPeople"
      privacy="high"
      contextthreatalgo="//@contextthreatalgo.0">
    <environment>noise</environment>
    <environment>sourrounded</environment>
  </contextinformation>
  <contextinformation
      xsi:type="modellingFrameworkContext4Authentication:RequiredContextInformation"
      context="Luminosity"
      contextthreatalgo="//@contextthreatalgo.1">
    <environment>darkness</environment>
  </contextinformation>
  <contextinformation
      xsi:type="modellingFrameworkContext4Authentication:RequiredContextInformation"
      context="Driving"
      privacy="medium"
      contextthreatalgo="//@contextthreatalgo.2">
    <environment>activity</environment>
  </contextinformation>
  <contextinformation
      xsi:type="modellingFrameworkContext4Authentication:RequiredContextInformation"
      context="Noise"
      privacy="medium"
      contextthreatalgo="//@contextthreatalgo.3">
    <environment>noise</environment>
  </contextinformation>
  <contextthreatalgo
      threatsituation="//@threatsituation.0"
      algo="algo1"
      requiredcontextinformation="//@contextinformation.0"/>
  <contextthreatalgo
      threatsituation="//@threatsituation.1"
      algo="algo2"
      requiredcontextinformation="//@contextinformation.1"/>
  <contextthreatalgo
      threatsituation="//@threatsituation.3"
      algo="lgo3"
      requiredcontextinformation="//@contextinformation.2"/>
  <contextthreatalgo
      threatsituation="//@threatsituation.2"
      algo="algo4"
      requiredcontextinformation="//@contextinformation.3"/>
  <threatsituation
      contextthreatalgo="//@contextthreatalgo.0"
      threatriskalgo="//@threatriskalgo.0"
      threat="Surrounded"/>
  <threatsituation
      contextthreatalgo="//@contextthreatalgo.1"
      threatriskalgo="//@threatriskalgo.1"
      threat="Dark"/>
  <threatsituation
      contextthreatalgo="//@contextthreatalgo.3"
      threatriskalgo="//@threatriskalgo.2"
      threat="Noisy"/>
  <threatsituation
      contextthreatalgo="//@contextthreatalgo.2"
      threatriskalgo="//@threatriskalgo.3"
      threat="Drives"/>
  <threatriskalgo
      threatsituation="//@threatsituation.0"
      risk="//@risk.0"
      algo="algo1"/>
  <threatriskalgo
      threatsituation="//@threatsituation.1"
      risk="//@risk.1"
      algo="algo2"/>
  <threatriskalgo
      threatsituation="//@threatsituation.2"
      risk="//@risk.2"
      algo="algo3"/>
  <threatriskalgo
      threatsituation="//@threatsituation.3"
      risk="//@risk.3"
      algo="algo4"/>
  <authenticationmechanism
      mechanism="fingerprint"
      privacy="high"
      risk="//@risk.0">
    <factor>being</factor>
    <factor>human</factor>
    <efficiencyToUse>noise</efficiencyToUse>
    <efficiencyToUse>darkness</efficiencyToUse>
    <efficiencyToUse>activity</efficiencyToUse>
    <efficiencyToUse>sourrounded</efficiencyToUse>
  </authenticationmechanism>
  <authenticationmechanism
      mechanism="face">
    <factor>being</factor>
    <efficiencyToUse>noise</efficiencyToUse>
    <efficiencyToUse>sourrounded</efficiencyToUse>
  </authenticationmechanism>
  <authenticationmechanism
      mechanism="SYK"
      risk="//@risk.1">
    <factor>knowledge</factor>
    <efficiencyToUse>noise</efficiencyToUse>
    <efficiencyToUse>darkness</efficiencyToUse>
    <efficiencyToUse>sourrounded</efficiencyToUse>
  </authenticationmechanism>
  <risk threatriskalgo="//@threatriskalgo.0"
      fearedEvent="R1"
      authenticationmechanism="//@authenticationmechanism.0">
    <factor>human</factor>
  </risk>
  <risk threatriskalgo="//@threatriskalgo.1"
      fearedEvent="R2"
      authenticationmechanism="//@authenticationmechanism.2"/>
  <risk threatriskalgo="//@threatriskalgo.2"
      fearedEvent="R3"/>
  <risk threatriskalgo="//@threatriskalgo.3"
      fearedEvent="R4"/>
</modellingFrameworkContext4Authentication:ModellingFramework>
